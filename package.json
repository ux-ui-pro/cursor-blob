{
  "name": "cursor-blob",
  "version": "1.1.0",
  "description": "Lightweight library for animated, interactive cursors using GSAP",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/cursor-blob.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/cursor-blob/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/cursor-blob",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "lint:js": "eslint src/**/*.{ts,js}",
    "lint:fix:js": "eslint src/**/*.{ts,js} --fix",
    "format:js": "prettier --write src/**/*.{ts,js}",
    "lint:fix": "yarn lint:fix:js && yarn format:js"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@rollup/plugin-terser": "0.4.4",
    "@types/node": "22.14.1",
    "@typescript-eslint/eslint-plugin": "8.31.0",
    "@typescript-eslint/parser": "8.31.0",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import": "2.31.0",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "typescript": "5.8.3",
    "vite": "6.3.2",
    "vite-plugin-dts": "4.5.3",
    "gsap": "3.12.7"
  },
  "keywords": [
    "cursor",
    "blob",
    "animation",
    "gsap",
    "mouse",
    "effect",
    "interactive",
    "tracking",
    "ui",
    "ux",
    "frontend",
    "visual",
    "custom"
  ]
}
